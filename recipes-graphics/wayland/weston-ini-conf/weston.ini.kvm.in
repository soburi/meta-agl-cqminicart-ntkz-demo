[core]
backend=drm-backend.so
require-input=false
modules=systemd-notify.so

[shell]
locking=true
panel-position=none

# A display is connected to HDMI-A-1
[output]
name=HDMI-A-1
agl-shell-app-id=@GUEST_VM1_IMAGE@

# A display is connected to HDMI-A-2
[output]
name=HDMI-A-2
transform=rotate-180
agl-shell-app-id=@GUEST_VM2_IMAGE@
